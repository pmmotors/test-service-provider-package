{
    "name": "pmmotors/google-apiclient",
    "description": "Google api php client wrapper with Cloud Platform and Laravel 4 & 5 support",
    "keywords": ["laravel", "google", "cloud platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pixel Motion"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5",
        "google/apiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "App\\":"app/",
            "pmmotors\\google-apiclient\\": "packages/pmmotors/google-apiclient/src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
